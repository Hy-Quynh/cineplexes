        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- ==========Preloader========== -->
    <!-- ==========Overlay========== -->
    <div class="overlay"></div>
    <a href="#0" class="scrollToTop">
        <i class="fas fa-angle-up"></i>
    </a>
    <!-- ==========Overlay========== -->

    <!-- ==========Movie-Section========== -->
    <div class="movie-facility padding-bottom padding-top">
        <div class="container">
          <div class="section-header-payment">
            <div class="confirm-payment">
              <span class="cate"><i class="far fa-check-circle"></i> Successfully</span>
              <h2 class="title">Thank you</h2>
              <p>Please check your mail for showtime info!</p>
              <a class="email" href="MailTo:<%= currentUser.email %>"><span><%= currentUser.email %></span></a>
